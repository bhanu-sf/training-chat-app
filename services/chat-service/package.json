{
  "name": "sl-chat-service-chat-service",
  "version": "0.0.1",
  "description": "chat-service",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "DOCKER_BUILDKIT=1 sudo docker build --build-arg NR_ENABLED=$NR_ENABLED_VALUE -t $IMAGE_REPO_NAME/\n    sl-chat-service-$npm_package_name:$npm_package_version .",
    "docker:run": "docker run -p 3000:3000 -d chat-service",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild && npm run openapi-spec",
    "start": "node -r ./dist/opentelemetry-registry.js -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "symlink-resolver": "symlink-resolver",
    "resolve-links": "npm run symlink-resolver build ./node_modules/@local",
    "docker:push": "sudo docker push $IMAGE_REPO_NAME/sl-chat-service-$npm_package_name:$npm_package_version",
    "docker:build:dev": "DOCKER_BUILDKIT=1 sudo docker build --build-arg NR_ENABLED=$NR_ENABLED_VALUE -t $IMAGE_REPO_NAME/\n    sl-chat-service-$npm_package_name:$IMAGE_TAG_VERSION .",
    "docker:push:dev": "sudo docker push $IMAGE_REPO_NAME/sl-chat-service-$npm_package_name:$IMAGE_TAG_VERSION",
    "coverage": "nyc npm run test",
    "dev": "nodemon --watch src -e ts --exec \"npm run start\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Bhanu Manthry <bhanuprasad.manthry@sourcefuse.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^4.1.1",
    "@loopback/core": "^3.1.1",
    "@loopback/repository": "^4.1.1",
    "@loopback/rest": "^11.1.1",
    "@loopback/rest-explorer": "^4.1.1",
    "@loopback/service-proxy": "^4.1.1",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/node": "^0.24.0",
    "@opentelemetry/plugin-dns": "^0.15.0",
    "@opentelemetry/plugin-http": "^0.18.2",
    "@opentelemetry/plugin-https": "^0.18.2",
    "@opentelemetry/plugin-pg": "^0.15.0",
    "@opentelemetry/plugin-pg-pool": "^0.15.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@sourceloop/chat-service": "^3.0.6",
    "@sourceloop/core": "^6.1.2",
    "dotenv": "^16.0.0",
    "dotenv-extended": "^2.9.0",
    "loopback-connector-postgresql": "^5.5.0",
    "loopback4-authentication": "^6.0.2",
    "loopback4-authorization": "^5.0.1",
    "prom-client": "^13.2.0",
    "swagger-stats": "^0.99.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@loopback/build": "^8.1.1",
    "@loopback/eslint-config": "^12.0.3",
    "@loopback/testlab": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^12.20.46",
    "eslint": "^8.10.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "symlink-resolver": "^0.2.1",
    "typescript": "~4.5.5"
  }
}
